{
    "compilerOptions": {
        "target": "es5",
        "esModuleInterop": true,
        "downlevelIteration": true,
        "suppressImplicitAnyIndexErrors": true,
        "strictNullChecks": false,
        "jsx": "react-jsx",
        "lib": [
            "es5", "es2015.core", "dom"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "dist"
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}